<div class="photo-form">
	<div class="container">
		<div class="app-nav">
		<%= link_to :back do %>
				<i class="material-icons">chevron_left</i>
			<% end %>
		</div>
		<h2>Edit Photograph</h2>
		<%= form_for(@photograph) do |p|%>
			<%= label_tag("Image") %>
			<%= p.text_field :image, :placeholder => 'image url' %>
			<%= label_tag("Name") %>
			<%= p.text_field :name, :placeholder => 'photography name' %>
			<%= label_tag("ISO") %>
			<%= p.text_field :iso, :placeholder => 'photography iso' %>
			<%= label_tag("Exposure") %>
			<%= p.text_field :exposure, :placeholder => 'photography exposure' %>
			<%= label_tag("ID") %>
			<%= p.text_field :project_id, :placeholder => 'project_id' %>
			<%= p.submit 'Update', :class => 'btn-submit'%>
		<% end %>
	</div>
</div>
<div class="clear-fix">
</div>